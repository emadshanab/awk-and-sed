awk '{print "https://" $0;}' pipedrive.txt > pipedrive1.txt (ADDS https infornt of line)
awk '{print $1}' remove everything after space
remove sed 's/https\?:\/\///' | sed 's/http\?:\/\///'
sed '/bar/d' remove whole line if bar is present
sed -i 's|http://|https://|g'  pipedrive1.txt
sed -e '/000/d' -e '/301/d' -e '/302/d' -e '/000/d' -e '/404/d' -e '/ remove multiple patterns
